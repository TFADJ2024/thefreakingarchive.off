{
  "format": "page",
  "id": "project",
  "label": "Project",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "students_name": {
        "type": "Text",
        "config": {
          "label": "First Name Last Name",
          "placeholder": ""
        }
      },
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "project_descrption": {
        "type": "StructuredText",
        "config": {
          "label": "Project Descrption",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "project_image": {
        "type": "Image",
        "config": {
          "label": "Project Image (Max 5 images)",
          "constraint": {
            "width": 1080
          },
          "thumbnails": [
            {
              "name": "pic 2",
              "width": 1080,
              "height": null
            },
            {
              "name": "pic 3",
              "width": 1080,
              "height": null
            }
          ]
        }
      },
      "project_title": {
        "type": "Text",
        "config": {
          "label": "Project Title",
          "placeholder": ""
        }
      },
      "date": {
        "type": "Text",
        "config": {
          "label": "Year Month",
          "placeholder": ""
        }
      },
      "project_link": {
        "type": "Link",
        "config": {
          "label": "Project link",
          "placeholder": "",
          "allowTargetBlank": false,
          "select": null,
          "allowText": false
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
